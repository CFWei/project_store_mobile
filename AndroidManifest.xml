<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.takenumbersystemstore"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:configChanges="keyboard|keyboardHidden|orientation"
            android:icon="@drawable/ic_launcher"
            android:screenOrientation="nosensor" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManageActivity" >
        </activity>
        <activity
            android:name="additem"
            android:windowSoftInputMode="adjustResize" >
        </activity>
        <activity android:name="itemfullscreen" >
        </activity>
        <activity android:name="edititem" >
        </activity>
        <activity
            android:name="modify_store_information"
            android:windowSoftInputMode="adjustResize" >
        </activity>
        <activity
            android:name=".Type2Activity"
            android:windowSoftInputMode="adjustResize" >
        </activity>
        <activity
            android:name=".Type2SettingPage"
            android:windowSoftInputMode="adjustResize" >
        </activity>
        <activity
            android:name=".Type2ItemList"
            android:windowSoftInputMode="adjustResize" >
        </activity>
        <activity
            android:name=".Type2Activity.Type2MainActivity"
            android:label="@string/title_activity_type2_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Type2MainActivity"
            android:label="@string/title_activity_type2_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>